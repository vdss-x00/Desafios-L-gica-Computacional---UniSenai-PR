programa {
  funcao inicio() {
    
    cadeia nome
    inteiro numero1, numero2, numero3

    escreva ("Primeiro número:")
    leia (numero1)

    escreva ("Segundo número:")
    leia (numero2)

    escreva ("Terceiro número:")
    leia (numero3)

    real media = (numero1 + numero2 + numero3) / 3

    escreva ("A média entre os números é: ", media) 



  }
}
